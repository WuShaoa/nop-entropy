      {
        "type": "group",
        "id": "row-tag-list",
        "body": [
          {
            "name": "tag-list",
            "label": "tag-listLabel",
            "clearValueOnHidden": false,
            "type": "input-tag",
            "disabled": true
          }
        ]
      },
      {
        "type": "group",
        "id": "row-image",
        "body": [
          {
            "name": "image",
            "label": "imageLabel",
            "clearValueOnHidden": false,
            "type": "static-image",
            "enlargeAble": true
          }
        ]
      },
      {
        "type": "group",
        "id": "row-file",
        "body": [
          {
            "name": "file",
            "label": "fileLabel",
            "clearValueOnHidden": false,
            "type": "group",
            "body": [
              {
                "type": "action",
                "visibleOn": "${file != null}",
                "actionType": "download",
                "label": "${fileComponentFileStatus.name}(${fileComponentFileStatus.fileSize})",
                "leve": "link",
                "api": {
                  "method": "get",
                  "url": "${file}",
                  "data": {},
                  "responseType": "blob"
                }
              }
            ]
          }
        ]
      },
      {
        "type": "group",
        "id": "row-file-list",
        "body": [
          {
            "name": "file-list",
            "label": "file-listLabel",
            "clearValueOnHidden": false,
            "type": "group",
            "body": [
              {
                "type": "action",
                "visibleOn": "${file-list != null}",
                "actionType": "dialog",
                "label": "查看下载列表",
                "dialog": {
                  "title": "文件列表",
                  "body": {
                    "type": "table",
                    "source": "${file-listComponentFileStatusList}",
                    "columns": [
                      {
                        "name": "name",
                        "label": "文件名称",
                        "level": "link"
                      },
                      {
                        "name": "fileSize",
                        "label": "文件大小"
                      },
                      {
                        "label": "下载",
                        "type": "operation",
                        "buttons": [
                          {
                            "label": "下载",
                            "type": "action",
                            "actionType": "download",
                            "api": {
                              "method": "get",
                              "url": "/f/download/$fileId",
                              "data": {},
                              "responseType": "blob"
                            }
                          }
                        ]
                      }
                    ]
                  },
                  "bodyClassName": "amis"
                }
              }
            ]
          }
        ]
      },
      {
        "type": "group",
        "id": "row-images",
        "body": [
          {
            "name": "images",
            "label": "imagesLabel",
            "clearValueOnHidden": false,
