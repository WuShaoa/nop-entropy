<?xml version="1.0" encoding="UTF-8" ?>
<view x:schema="/nop/schema/xui/xview.xdef" bizObjName="NopAuthExtLogin" xmlns:i18n-en="i18n-en"
      xmlns:x="/nop/schema/xdsl.xdef" xmlns:j="j" xmlns:c="c" xmlns:gql="gql" xmlns:view-gen="view-gen" xmlns:xpl="xpl">

    <objMeta>/nop/auth/model/NopAuthExtLogin/NopAuthExtLogin.xmeta</objMeta>

    <controlLib>/nop/web/xlib/control.xlib</controlLib>

    <x:gen-extends>
        <view-gen:DefaultViewGenExtends xpl:lib="/nop/web/xlib/view-gen.xlib"/>
    </x:gen-extends>

    <x:post-extends>
        <view-gen:DefaultViewPostExtends xpl:lib="/nop/web/xlib/view-gen.xlib"/>
    </x:post-extends>

    <grids>
        <grid id="list" x:abstract="true">
            <cols>

                <!--登录类型-->
                <col id="loginType" mandatory="true" sortable="true"/>

                <!--登录标识-->
                <col id="extId" mandatory="true" sortable="true"/>

                <!--登录密码-->
                <col id="credential" sortable="true"/>

                <!--是否已验证-->
                <col id="verified" mandatory="true" sortable="true"/>

                <!--上次登录时间-->
                <col id="lastLoginTime" sortable="true"/>

                <!--上次登录IP-->
                <col id="lastLoginIp" sortable="true"/>

                <!--创建人-->
                <col id="createdBy" mandatory="true" sortable="true" x:abstract="true"/>

                <!--创建时间-->
                <col id="createTime" mandatory="true" sortable="true" x:abstract="true"/>

                <!--修改人-->
                <col id="updatedBy" mandatory="true" sortable="true" x:abstract="true"/>

                <!--修改时间-->
                <col id="updateTime" mandatory="true" sortable="true" x:abstract="true"/>

                <!--备注-->
                <col id="remark" sortable="true" x:abstract="true"/>
            </cols>
        </grid>
        <grid id="pick-list" x:prototype="list" x:abstract="true"/>
    </grids>

    <forms>
        <form id="view" editMode="view" title="查看-扩展登录方式" i18n-en:title="View Ext Login">
            <layout>
 loginType[登录类型] extId[登录标识]
 credential[登录密码] verified[是否已验证]
 lastLoginTime[上次登录时间] lastLoginIp[上次登录IP]
 createdBy[创建人] createTime[创建时间]
 updatedBy[修改人] updateTime[修改时间]
 remark[备注] 
</layout>
        </form>
        <form id="add" editMode="add" title="新增-扩展登录方式" i18n-en:title="Add Ext Login" x:prototype="edit"/>
        <form id="edit" editMode="edit" title="编辑-扩展登录方式" i18n-en:title="Edit Ext Login">
            <layout>
 loginType[登录类型]
 extId[登录标识]
 credential[登录密码]
 verified[是否已验证]
 lastLoginTime[上次登录时间]
 lastLoginIp[上次登录IP]
 remark[备注]
</layout>
        </form>
        <form id="query" editMode="query" title="查询条件" i18n-en:title="Query Condition" x:abstract="true">
            <layout/>
        </form>
        <form id="asideFilter" editMode="query" x:abstract="true" submitOnChange="true">
            <layout/>
        </form>
        <form id="batchUpdate" editMode="edit" x:abstract="true" title="修改-扩展登录方式" i18n-en:title="Update Ext Login">
            <layout/>
        </form>
    </forms>

    <pages>
        <crud name="view-list" grid="list" asideFilterForm="asideFilter" filterForm="query" x:abstract="true">
            <table autoFillHeight="true">
                <api url="@query:NopAuthExtLogin__findPage" gql:selection="{@pageSelection}"/>
            </table>
        </crud>
        <crud name="main" grid="list" asideFilterForm="asideFilter" filterForm="query" x:abstract="true">
            <table autoFillHeight="true">
                <api url="@query:NopAuthExtLogin__findPage" gql:selection="{@pageSelection}"/>
            </table>
            <listActions>
                <action id="batch-delete-button" label="@i18n:common.batchDelete" batch="true">
                    <api url="@mutation:NopAuthExtLogin__batchDelete?ids=$ids"/>
                    <confirmText>@i18n:common.confirmBatchDelete</confirmText>
                </action>
                <action id="add-button" level="primary" label="@i18n:common.add" icon="fa fa-plus pull-left">
                    <dialog page="add"/>
                </action>
            </listActions>
            <rowActions>
                <action id="row-view-button" level="primary" label="@i18n:common.view">
                    <dialog page="view"/>
                </action>
                <action id="row-update-button" level="primary" label="@i18n:common.edit" x:abstract="true">
                    <dialog page="update"/>
                </action>
                <action id="row-delete-button" level="danger" label="@i18n:common.delete" x:abstract="true">
                    <api url="@mutation:NopAuthExtLogin__delete?id=$id"/>
                    <confirmText>@i18n:common.confirmDelete</confirmText>
                </action>
                <actionGroup id="row-more-button" label="@i18n:common.more" level="primary">
                    <action id="row-update-button" label="@i18n:common.edit">
                        <dialog page="update"/>
                    </action>
                    <action id="row-delete-button" label="@i18n:common.delete">
                        <api url="@mutation:NopAuthExtLogin__delete?id=$id"/>
                        <confirmText>@i18n:common.confirmDelete</confirmText>
                    </action>
                </actionGroup>
            </rowActions>
        </crud>
        <picker name="picker" grid="pick-list" asideFilterForm="asideFilter" filterForm="query" x:abstract="true">
            <table>
                <api url="@query:NopAuthExtLogin__findPage" gql:selection="{@pageSelection}"/>
            </table>
        </picker>
        <simple name="add" form="add">
            <api url="@mutation:NopAuthExtLogin__save/id"/>
        </simple>
        <simple name="view" form="view">
            <initApi url="@query:NopAuthExtLogin__get?id=$id" gql:selection="{@formSelection}"/>
        </simple>
        <simple name="update" form="edit">
            <initApi url="@query:NopAuthExtLogin__get?id=$id" gql:selection="{@formSelection}"/>
            <api url="@mutation:NopAuthExtLogin__update/id?id=$id" withFormData="true"/>
        </simple>
    </pages>
</view>